        <script type="text/javascript">
        $("#SignOut").click(function () {

            $.ajax({


                type: "POST",
               url: '@Url.Action("LogOff", "Account")',
                success: function (data) {
                    if (data == true) {
                        window.location.href ='@Url.Action("Login", "Account")'
                    }
                    else {

